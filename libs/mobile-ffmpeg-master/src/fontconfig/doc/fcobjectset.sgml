    <refentry id="FcObjectSetCreate">
	<refmeta>
	    <refentrytitle>FcObjectSetCreate</refentrytitle>
	    <manvolnum>3</manvolnum>
	    <refmiscinfo class="software">Fontconfig &version;</refmiscinfo>
	</refmeta>
	<refnamediv>
	    <refname>FcObjectSetCreate</refname>
