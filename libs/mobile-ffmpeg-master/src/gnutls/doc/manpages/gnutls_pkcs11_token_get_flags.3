.SH NAME
gnutls_pkcs11_token_get_flags \- API function
.SH SYNOPSIS
.B #include <gnutls/pkcs11.h>
.sp
.BI "int gnutls_pkcs11_token_get_flags(const char * " url ", unsigned int * " flags ");"
.SH ARGUMENTS
.IP "const char * url" 12
